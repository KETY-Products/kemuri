{
  "name": "mock-tool",
  "version": "1.0.0",
  "description": "Mock Tool",
  "private": true,
  "bin": {
    "builder": "bin/bin/builder.js",
    "html-builder": "bin/bin/html-builder.js",
    "css-builder": "bin/css-builder.js",
    "js-builder": "bin/js-builder.js",
    "server": "bin/server.js"
  },
  "type": "module",
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "browser-sync": "^2.29.3",
    "chalk": "^5.3.0",
    "cosmiconfig": "^8.3.6",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "js-beautify": "^1.14.9",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-frontmatter": "^2.0.1",
    "nunjucks": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "sass": "^1.69.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unist-util-find": "^3.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-find-all-after": "^5.0.0",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.27.3",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.2",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash": "^4.14.200",
    "@types/mdast": "^4.0.2",
    "@types/node": "^20.8.7",
    "@types/nunjucks": "^3.2.5",
    "@types/yargs": "^17.0.29",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsx": "^3.14.0"
  },
  "scripts": {
    "server": "tsx lib/server.ts",
    "builder": "yarn builder:build",
    "builder:watch": "tsx lib/builder.ts --html --css --js --watch",
    "builder:build": "tsx lib/builder.ts --html --css --js",
    "html-builder:watch": "tsx lib/html-builder.ts --watch",
    "html-builder:build": "tsx lib/html-builder.ts",
    "css-builder": "yarn css-builder:build",
    "css-builder:watch": "tsx lib/css-builder.ts --watch",
    "css-builder:build": "tsx lib/css-builder.ts",
    "js-builder": "yarn js-builder:build",
    "js-builder:watch": "tsx lib/js-builder.ts --watch",
    "js-builder:build": "tsx lib/js-builder.ts",
    "snippet-builder:build": "tsx lib/snippet-builder.ts",
    "cleanup": "rimraf public/*",
    "cleanup:assets": "run-p site:cleanup:assets:*",
    "cleanup:assets:js": "rimraf public/assets/js/*",
    "cleanup:assets:css": "rimraf public/assets/css/*",
    "----------": "",
    "build": "rollup -c"
  }
}
