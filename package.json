{
  "name": "kemuri",
  "version": "0.6.5",
  "description": "Mockup builder",
  "private": true,
  "type": "module",
  "bin": {
    "kemuri": "bin/kemuri.js",
    "kemuri-server": "bin/kemuri-server.js",
    "kemuri-screenshot": "bin/kemuri-screenshot.js",
    "kemuri-snippet": "bin/kemuri-snippet.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "autoprefixer": "^10.4.17",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cosmiconfig": "^9.0.0",
    "cpx2": "^7.0.1",
    "dotenv": "^16.4.4",
    "duplexer3": "^1.0.0",
    "editorconfig": "^2.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "js-beautify": "^1.15.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^23.2.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-frontmatter": "^2.0.1",
    "micromark-extension-frontmatter": "^2.0.0",
    "micromatch": "^4.0.5",
    "nunjucks": "^3.2.4",
    "playwright": "^1.41.2",
    "postcss": "^8.4.35",
    "rollup": "^4.12.0",
    "sass": "^1.71.0",
    "shell-quote": "^1.8.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unist-util-find": "^3.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-find-all-after": "^5.0.0",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@fullcalendar/bootstrap": "^6.1.10",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/google-calendar": "^6.1.10",
    "@fullcalendar/icalendar": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/list": "^6.1.10",
    "@fullcalendar/multimonth": "^6.1.10",
    "@fullcalendar/scrollgrid": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@types/browser-sync": "^2.29.0",
    "@types/cpx": "^1.5.5",
    "@types/duplexer3": "^0.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.11.19",
    "@types/nunjucks": "^3.2.6",
    "@types/resolve": "^1.20.6",
    "@types/shell-quote": "^1.7.5",
    "@types/yargs": "^17.0.32",
    "bootstrap": "^5.3.2",
    "bulma": "^0.9.4",
    "dayjs": "^1.11.10",
    "ical.js": "^1.5.0",
    "npm-check-updates": "^16.14.15",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "vue": "^3.4.19"
  },
  "scripts": {
    "builder": "run-s builder:build",
    "builder:watch": "tsx lib/builder.ts --html --css --js --copy --watch --server",
    "builder:build": "tsx lib/builder.ts --html --css --js --copy",
    "builder:init": "tsx lib/builder.ts --init",
    "server": "tsx lib/server.ts",
    "screenshot": "tsx lib/screenshot.ts",
    "snippet-builder:watch": "tsx lib/snippet-builder.ts --watch",
    "snippet-builder:build": "tsx lib/snippet-builder.ts --clean",
    "test": "run-p test:html test:js test:css test:copy test:snippet",
    "test:html": "run-s cleanup:test-output:html test:html:*",
    "test:html:default": "tsx lib/builder.ts --config=tests/html_test_default.yml",
    "test:html:no-beautify": "tsx lib/builder.ts --config=tests/html_test_no-beautify.yml",
    "test:js": "run-s cleanup:test-output:js test:js:*",
    "test:js:default": "tsx lib/builder.ts --config=tests/js_test_default.yml",
    "test:js:beautify": "tsx lib/builder.ts --config=tests/js_test_beautify.yml",
    "test:js:minify": "tsx lib/builder.ts --config=tests/js_test_minify.yml",
    "test:js:beautify_minify": "tsx lib/builder.ts --config=tests/js_test_beautify_minify.yml",
    "test:css": "run-s cleanup:test-output:css test:css:*",
    "test:css:default": "tsx lib/builder.ts --config=tests/css_test_default.yml",
    "test:css:beautify": "tsx lib/builder.ts --config=tests/css_test_beautify.yml",
    "test:css:beautify_compressed": "tsx lib/builder.ts --config=tests/css_test_beautify_compressed.yml",
    "test:css:style-expanded": "tsx lib/builder.ts --config=tests/css_test_style_expanded.yml",
    "test:css:style-compressed": "tsx lib/builder.ts --config=tests/css_test_style_compressed.yml",
    "test:css:generate_index": "tsx lib/builder.ts --config=tests/css_test_generate_index.yml",
    "test:copy": "run-s cleanup:test-output:copy test:copy:*",
    "test:copy:default": "tsx lib/builder.ts --config=tests/copy_test_default.yml",
    "test:snippet": "run-s cleanup:test-output:snippet test:snippet:*",
    "test:snippet:default": "tsx lib/snippet-builder.ts --config=tests/snippet_test_default.yml",
    "test:screenshot:prepare": "run-s test:screenshot:prepare:*",
    "test:screenshot:prepare:cleanup-fixture": "rimraf tests/output/html/screenshot/*",
    "test:screenshot:prepare:cleanup-screenshots": "npm run cleanup:test-output:screenshot",
    "test:screenshot:prepare:build": "tsx lib/builder.ts --config=tests/screenshot_test_prepare.yml  --html --css --js --server",
    "test:screenshot:test": "run-s test:screenshot:test:*",
    "test:screenshot:test:default": "tsx lib/screenshot.ts --config=tests/screenshot_test_default.yml",
    "test:screenshot:test:flat": "tsx lib/screenshot.ts --config=tests/screenshot_test_flat.yml",
    "test:screenshot:test:multi_device": "tsx lib/screenshot.ts --config=tests/screenshot_test_multi_device.yml",
    "test:screenshot:test:multi_device_and_flat": "tsx lib/screenshot.ts --config=tests/screenshot_test_multi_device_flat.yml",
    "test:screenshot:test:element": "tsx lib/screenshot.ts --config=tests/screenshot_test_element.yml",
    "cleanup:test-output": "run-p cleanup:test-output:*",
    "cleanup:test-output:html": "rimraf tests/output/html/*",
    "cleanup:test-output:css": "rimraf tests/output/css/*",
    "cleanup:test-output:js": "rimraf tests/output/js/*",
    "cleanup:test-output:copy": "rimraf tests/output/copy/*",
    "cleanup:test-output:screenshot": "rimraf tests/output/screenshot/*",
    "cleanup:test-output:snippet": "rimraf tests/output/snippet/*",
    "----------": "",
    "npm-check-updates": "npm-check-updates",
    "npm-check-updates-and-upgrade": "npm-check-updates --upgrade",
    "bin-build": "run-s bin-build:*",
    "bin-build:build": "rollup -c",
    "bin-build:chmod": "chmod 0755 bin/*.js"
  }
}
