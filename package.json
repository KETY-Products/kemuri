{
  "name": "mock-tool",
  "version": "1.0.0",
  "description": "Mock Tool",
  "scripts": {
    "server": "npm run watch:dist",
    "watch": "run-p watch:**",
    "watch:nunjucks": "node bin/build_html.js --watch",
    "watch:scss": "node bin/build_css.js --watch",
    "watch:js": "tsc --watch",
    "watch:dist": "browser-sync dist --watch",
    "build": "run-s cleanup:dist build:**",
    "build:html": "node bin/build_html.js --production",
    "build:css": "node bin/build_css.js --production",
    "build:js": "tsc",
    "cleanup": "run-p cleanup:**",
    "cleanup:dist": "rimraf dist/*"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/node": "18.11.7",
    "browser-sync": "^2.27.10",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.3",
    "editorconfig": "^1.0.0",
    "js-beautify": "^1.14.6",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "yaml": "^2.1.3"
  },
  "dependencies": {
    "bignumber.js": "^9.1.1",
    "dayjs": "^1.11.7"
  }
}
