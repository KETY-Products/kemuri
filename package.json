{
  "name": "kemuri",
  "version": "0.4.1",
  "description": "Mockup builder",
  "private": true,
  "type": "module",
  "bin": {
    "kemuri": "bin/kemuri.js",
    "kemuri-server": "bin/kemuri-server.js",
    "kemuri-screenshot": "bin/kemuri-screenshot.js",
    "kemuri-snippet": "bin/kemuri-snippet.js"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "browser-sync": "^2.29.3",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cosmiconfig": "^8.3.6",
    "cpx": "github:mysticatea/cpx",
    "dotenv": "^16.3.1",
    "duplexer3": "^1.0.0",
    "editorconfig": "^2.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "js-beautify": "^1.14.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-frontmatter": "^2.0.1",
    "micromark-extension-frontmatter": "^2.0.0",
    "micromatch": "^4.0.5",
    "nunjucks": "^3.2.4",
    "playwright": "^1.40.1",
    "resolve": "^1.22.8",
    "rollup": "^4.9.1",
    "sass": "^1.69.5",
    "shell-quote": "^1.8.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unist-util-find": "^3.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-find-all-after": "^5.0.0",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.29.0",
    "@types/cpx": "^1.5.5",
    "@types/duplexer3": "^0.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.10.5",
    "@types/nunjucks": "^3.2.6",
    "@types/resolve": "^1.20.6",
    "@types/shell-quote": "^1.7.5",
    "@types/yargs": "^17.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rollup-plugin-cleandir": "^2.0.0",
    "tsx": "^3.14.0"
  },
  "scripts": {
    "builder": "run-s builder:build",
    "builder:watch": "tsx lib/builder.ts --html --css --js --copy --watch --server",
    "builder:build": "tsx lib/builder.ts --html --css --js --copy --clean",
    "builder:init": "tsx lib/builder.ts --init",
    "server": "tsx lib/server.ts",
    "screenshot": "run-s screenshot:*",
    "screenshot:rebuild": "run-s builder:build bin-build:build",
    "screenshot:screenshot": "node bin/kemuri-screenshot.js --clean",
    "snippet-builder:watch": "tsx lib/snippet-builder.ts --watch",
    "snippet-builder:build": "tsx lib/snippet-builder.ts --clean",
    "----------": "",
    "bin-build": "run-s bin-build:*",
    "bin-build:build": "rollup -c",
    "bin-build:chmod": "chmod 0755 bin/*.js"
  }
}
