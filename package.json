{
  "name": "mock-tool",
  "version": "1.0.0",
  "description": "Mock Tool",
  "scripts": {
    "server": "npm run watch:dist",
    "watch": "run-p watch:**",
    "watch:nunjucks": "node bin/build_html.js --watch",
    "watch:scss": "node bin/build_css.js --watch",
    "watch:js": "tsc --watch",
    "watch:dist": "browser-sync dist --watch",
    "build": "run-s cleanup:dist build:**",
    "build:html": "node bin/build_html.js --production",
    "build:css": "node bin/build_css.js --production",
    "build:js": "tsc",
    "cleanup": "run-p cleanup:**",
    "cleanup:dist": "rimraf dist/*"
  },
  "devDependencies": {
    "@types/jquery": "3.*",
    "@types/node": "18.*",
    "browser-sync": "2.*",
    "chokidar": "3.*",
    "dotenv": "16.*",
    "editorconfig": "1.*",
    "js-beautify": "1.*",
    "npm-run-all": "4.*",
    "nunjucks": "3.*",
    "rimraf": "3.*",
    "sass": "1.*",
    "typescript": "4.*",
    "yaml": "2.*"
  },
  "dependencies": {
    "bignumber.js": "9.*",
    "dayjs": "1.*"
  }
}
