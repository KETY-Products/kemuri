{
  "name": "kemuri",
  "version": "0.5.2",
  "description": "Mockup builder",
  "private": true,
  "type": "module",
  "bin": {
    "kemuri": "bin/kemuri.js",
    "kemuri-server": "bin/kemuri-server.js",
    "kemuri-screenshot": "bin/kemuri-screenshot.js",
    "kemuri-snippet": "bin/kemuri-snippet.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cosmiconfig": "^9.0.0",
    "cpx2": "^7.0.1",
    "dotenv": "^16.3.1",
    "duplexer3": "^1.0.0",
    "editorconfig": "^2.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "js-beautify": "^1.14.11",
    "js-yaml": "^4.1.0",
    "jsdom": "^23.2.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-frontmatter": "^2.0.1",
    "micromark-extension-frontmatter": "^2.0.0",
    "micromatch": "^4.0.5",
    "npm-check-updates": "^16.14.12",
    "nunjucks": "^3.2.4",
    "playwright": "^1.40.1",
    "resolve": "^1.22.8",
    "rollup": "^4.9.4",
    "sass": "^1.69.7",
    "shell-quote": "^1.8.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unist-util-find": "^3.0.0",
    "unist-util-find-after": "^5.0.0",
    "unist-util-find-all-after": "^5.0.0",
    "unist-util-find-all-between": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.29.0",
    "@types/cpx": "^1.5.5",
    "@types/duplexer3": "^0.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.11.0",
    "@types/nunjucks": "^3.2.6",
    "@types/resolve": "^1.20.6",
    "@types/shell-quote": "^1.7.5",
    "@types/yargs": "^17.0.32",
    "bootstrap": "^5.3.2",
    "bulma": "^0.9.4",
    "dayjs": "^1.11.10",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "vue": "^3.4.13"
  },
  "scripts": {
    "builder": "run-s builder:build",
    "builder:watch": "tsx lib/builder.ts --html --css --js --copy --watch --server",
    "builder:build": "tsx lib/builder.ts --html --css --js --copy",
    "builder:init": "tsx lib/builder.ts --init",
    "server": "tsx lib/server.ts",
    "screenshot": "tsx lib/screenshot.ts",
    "snippet-builder:watch": "tsx lib/snippet-builder.ts --watch",
    "snippet-builder:build": "tsx lib/snippet-builder.ts --clean",
    "test": "run-p test:js test:css test:snippet",
    "test:js": "run-s cleanup:test-output:js test:js:*",
    "test:js:default": "tsx lib/builder.ts --config=tests/js_test_default.yml --js",
    "test:js:beautify": "tsx lib/builder.ts --config=tests/js_test_beautify.yml --js",
    "test:js:minify": "tsx lib/builder.ts --config=tests/js_test_minify.yml --js",
    "test:js:beautify_minify": "tsx lib/builder.ts --config=tests/js_test_beautify_minify.yml --js",
    "test:css": "run-s cleanup:test-output:css test:css:*",
    "test:css:default": "tsx lib/builder.ts --config=tests/css_test_default.yml --css",
    "test:css:beautify": "tsx lib/builder.ts --config=tests/css_test_beautify.yml  --css",
    "test:css:beautify_compressed": "tsx lib/builder.ts --config=tests/css_test_beautify_compressed.yml  --css",
    "test:css:style-expanded": "tsx lib/builder.ts --config=tests/css_test_style_expanded.yml --css",
    "test:css:style-compressed": "tsx lib/builder.ts --config=tests/css_test_style_compressed.yml --css",
    "test:snippet": "run-s cleanup:test-output:snippet test:snippet:*",
    "test:snippet:default": "tsx lib/snippet-builder.ts --config=tests/snippet_test_default.yml",
    "cleanup:test-output": "run-p cleanup:test-output:*",
    "cleanup:test-output:css": "rimraf tests/output/css/*",
    "cleanup:test-output:js": "rimraf tests/output/js/*",
    "cleanup:test-output:snippet": "rimraf tests/output/snippet/*",
    "----------": "",
    "npm-check-updates": "npx npm-check-updates -u",
    "bin-build": "run-s bin-build:*",
    "bin-build:build": "rollup -c",
    "bin-build:chmod": "chmod 0755 bin/*.js"
  }
}
